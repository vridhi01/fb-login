<?xml version="1.0"?>


<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
<update handle="customer_account"/>
<head>
    <title>Social Login</title>
</head> 
<body>
    
    <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Social Login</argument>
            </action>
     </referenceBlock>
     <referenceContainer name="content">
        <block class="Excellence\FacebookLogin\Block\FacebookLogin" ifconfig="facebooklogin/facebook/is_enabled" name="facebooklogin.remove" template="facebooklogin_remove.phtml">
                
            </block>
    </referenceContainer>
</body>
</page>